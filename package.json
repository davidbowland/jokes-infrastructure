{
  "name": "dbowland-pulumi-jokes",
  "version": "1.0.0",
  "private": true,
  "description": "dbowland.com development infrastructure",
  "author": "David Bowland",
  "engines": {
    "node": "^16.0.0"
  },
  "scripts": {
    "deploy": "./scripts/deploy.sh",
    "preview": "./scripts/preview.sh",
    "lint": "prettier --write . && eslint --fix .",
    "refresh": "./scripts/refresh.sh"
  },
  "dependencies": {
    "@pulumi/aws": "^4.0.0",
    "@pulumi/awsx": "^0.30.0",
    "@pulumi/pulumi": "^3.0.0",
    "aws-sdk": "^2.912.0"
  },
  "devDependencies": {
    "@types/node": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "eslint --fix ."
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
