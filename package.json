{
  "name": "jokes-infrastrcuture",
  "version": "1.1.0",
  "private": true,
  "description": "Infrastructure for jokes project",
  "author": "David Bowland",
  "engines": {
    "node": "^16.0.0"
  },
  "scripts": {
    "deploy": "./scripts/deploy.sh",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install ; fi",
    "preview": "./scripts/preview.sh",
    "lint": "prettier --write . && eslint --fix .",
    "refresh": "./scripts/refresh.sh"
  },
  "dependencies": {
    "@pulumi/aws": "^4.0.0",
    "@pulumi/awsx": "^0.30.0",
    "@pulumi/pulumi": "^3.0.0",
    "@types/node": "^15.6.1",
    "aws-sdk": "^2.912.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidbowland/jokes-infrastructure.git"
  },
  "bugs": {
    "url": "https://github.com/davidbowland/jokes-infrastructure/issues"
  },
  "homepage": "https://github.com/davidbowland/jokes-infrastructure#readme"
}
